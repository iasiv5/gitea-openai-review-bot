# Gitea 配置
GITEA_URL=https://your-gitea.com
GITEA_BOT_TOKEN=your-gitea-bot-token
GITEA_WEBHOOK_SECRET=your-webhook-secret

# OpenAI 配置
OPENAI_API_KEY=your-openai-api-key

# 可选：Redis 配置（用于缓存）
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# 日志级别
LOG_LEVEL=info

# 其他配置
MAX_CONCURRENT_REVIEWS=5
ENABLE_CACHE=true